{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd mueblesar-web && npm install && npm run build",
  "outputDirectory": "mueblesar-web/.next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.amobly.com.ar/api/:path*"
    }
  ]
}
